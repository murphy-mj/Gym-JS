{{> menu_trainer id="alltrainers"}}
<div class="ui cards">
{{#each trainersData}}
  <div class="ui card">
  
    <div class="image">
       <img class="ui small image" src="{{photo}}">
    </div>
  
    <div class="content">
       <a class="header"> {{firstName}} {{lastName}}</a>
    </div>
  <div class="extra content">
    <div class="ui two buttons">
    <a href="/trainer/group_clients/{{id}}"><div class="ui basic green button">View Clients</div> </a><br> 
    <a href="/trainer/group_about/{{id}}"><div class="ui basic blue button">About Trainer</div> </a>
    </div>
  </div>
    
</div>
{{/each}}
</div>
  
